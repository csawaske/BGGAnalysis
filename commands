jq -c '{id : .item.primaryname.nameid, name : .item.primaryname.name, subtype : .item.subtypename, year: .item.yearpublished, mintime : .item.minplaytime, maxtime : .item.maxplaytime, age : .item.minage, minplayers : .item.minplayers, maxplayers : .item.maxplayers, categories : .item.boardgamecategory, mechanics : .item.boardgamemechanic, publishers : .item.boardgamepublisher, designers : .item.boardgamedesigner, artists : .item.boardgameartist, expands : .item.expandsboardgame, videogame : .item.videogamebg, usrage : .item.polls.playerage, usrplayers : .item.polls.userplayers, weight : .item.polls.boardgameweight.averageweight, stats : .item.stats}' boardgamegeek.json > pruned.json



